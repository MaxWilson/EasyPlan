{
    "manifestVersion": 1,
    "id": "EasyPlan",
    "publisher": "MaxWilson",
    "version": "1.0.7",
    "name": "EasyPlan",
    "description": "Track events important to your team, view and manage days off, quickly see when sprints start and end, and more.",
    "public": false,
    "categories": ["Azure Boards"],
    "baseUri": "https://localhost:44300",
    "scopes": ["vso.work_write", "vso.profile", "vso.project"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "EasyPlan",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "EasyPlan",
                "uri": "easyPlan.html"
            }
        }
    ],
    "files": [
        {
            "path": "dist",
            "addressable": true
        }
    ]
}