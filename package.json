{
    "private": true,
    "scripts": {
        "prestart": "dotnet tool restore",
        "start": "dotnet fable watch ./src -s --run webpack-dev-server",
        "prebuild": "dotnet tool restore",
        "build": "erase dist/* && dotnet fable ./src && webpack",
        "test:live": "dotnet fable watch tests --run webpack-dev-server --config ./webpack.tests.js",
        "build:test": "dotnet fable tests && webpack --config webpack.tests.js",
        "pretest": "dotnet tool restore",
        "test": "dotnet fable tests --outDir dist/tests && mocha dist/tests -r esm",
        "publish": "npx tfx-cli extension create --rev-version",
        "publishOsgs": "erase *.vsix && npx tfx-cli extension create --manifests osgsEasyPlan.json --rev-version"
    },
    "dependencies": {
        "bulma": "^0.9.3",
        "date-fns": "^2.28.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "tfx-cli": "^0.12.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/runtime": "^7.17.9",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
        "@webpack-cli/serve": "^1.6.1",
        "azure-devops-extension-api": "^1.158.0",
        "azure-devops-extension-sdk": "^2.0.11",
        "babel-loader": "^8.2.5",
        "copy-webpack-plugin": "^10.2.4",
        "core-js": "^3.22.2",
        "css-loader": "^6.7.1",
        "dotenv-webpack": "^7.1.0",
        "esm": "^3.2.25",
        "file-loader": "^6.2.0",
        "graceful-fs": "^4.2.10",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.0",
        "mocha": "^9.2.2",
        "react-drag-drop-container": "^6.1.1",
        "react-konva": "^17.0.2-5",
        "react-refresh": "^0.12.0",
        "sass": "^1.51.0",
        "sass-loader": "^12.6.0",
        "source-map-loader": "^3.0.1",
        "style-loader": "^3.3.1",
        "terser": "^5.13.0",
        "vss-web-extension-sdk": "^5.141.0",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1"
    }
